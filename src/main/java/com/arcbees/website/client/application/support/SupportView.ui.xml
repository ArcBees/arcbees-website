<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="res" type="com.arcbees.website.client.resources.AppResources"/>
    <ui:with field="page" type="com.arcbees.website.client.resources.PageSupportResources"/>
    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>
    <ui:with field="animations" type="com.arcbees.website.client.resources.AnimationsResources"/>

    <ui:import field="com.arcbees.website.client.NameTokens.*"/>

    <g:HTMLPanel styleName="{page.style.support}">

        <h1 class="{fonts.icons.iconArcbees} {res.style.withLogo}">
            <span>Support</span>
        </h1>

        <section class="{res.style.stripe} {res.style.header} {page.style.header} {res.style.colored}">
            <div class="{page.style.featuredIcons}">

                <i class="{fonts.icons.iconGwtp} {page.style.iconGwtp} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconJukito} {page.style.iconJukito} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconChosen} {page.style.iconChosen} {animations.style.animation}"></i>

                <i class="{fonts.icons.iconhexaEmpty} {page.style.iconhexa1} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconhexaEmpty} {page.style.iconhexa2} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconhexaEmpty} {page.style.iconhexa3} {animations.style.animation}"></i>

            </div>
        </section>

        <section class="{res.style.stripe} {res.style.dark} {res.style.triangleBottom} {res.style.nextColored}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper} {res.style.focusBlock}">
                <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_push_1_4} {res.grid.col_m_full} {res.grid.col_m_push_0}">

                    <h2 class="{res.style.stripeTitle}">Support
                        <span>packages</span>
                    </h2>
                    <p>ArcBees is pleased to offer a variety of supports options to its clients. Pick the one that
                        fits you best. You can also contact us for a custom made kit just for you.
                    </p>
                </div>
            </div>
        </section>

        <section>
            <div class="{res.style.stripe} {res.style.colored}">
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper} {res.style.centered} {page.style.offers}">

                    <article class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_t_full} {page.style.plan}">
                        <header>
                            <h3>Basic</h3>
                            <p class="{page.style.price}">
                                <strong>199.95</strong>
                                $/year
                            </p>
                        </header>
                        <section>
                            <div class="{page.style.includes}">
                                <div class="{page.style.developers}">
                                    <p>1
                                        <span>Developers</span>
                                    </p>
                                </div>
                                <div class="{page.style.advisory}">
                                    <p>1
                                        <span>Advisory Hours</span>
                                    </p>
                                </div>
                            </div>
                            <div class="{page.style.hours}">
                                <p>
                                    48 hours
                                    <span>Response time</span>
                                </p>
                            </div>
                        </section>
                    </article>

                    <article
                            class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_t_full} {page.style.plan} {page.style.bestbuy}">
                        <div class="{page.style.popChoice}">
                            <h4>popular choice</h4>
                        </div>
                        <header>
                            <h3>Professional</h3>
                            <p class="{page.style.price}">
                                <strong>499.95</strong>
                                $/year
                            </p>
                        </header>
                        <section>
                            <div class="{page.style.includes}">
                                <div class="{page.style.developers}">
                                    <p>5
                                        <span>Developers</span>
                                    </p>

                                </div>
                                <div class="{page.style.advisory}">
                                    <p>3
                                        <span>Advisory Hours</span>
                                    </p>

                                </div>
                            </div>
                            <div class="{page.style.hours}">
                                <p>
                                    24 hours
                                    <span>Response time</span>
                                </p>
                            </div>
                        </section>
                    </article>

                    <article class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_t_full} {page.style.plan}">
                        <header>
                            <h3>Business</h3>
                            <p class="{page.style.price}">
                                <strong>2 999.95</strong>
                                $/year
                            </p>
                        </header>
                        <section>
                            <div class="{page.style.includes}">
                                <div class="{page.style.developers}">
                                    <p>25
                                        <span>Developers</span>
                                    </p>
                                </div>
                                <div class="{page.style.advisory}">
                                    <p>16
                                        <span>Advisory Hours</span>
                                    </p>
                                </div>
                            </div>
                            <div class="{page.style.hours}">
                                <p>
                                    2 hours
                                    <span>Response time</span>
                                </p>
                            </div>
                        </section>
                    </article>
                </div>
            </div>
        </section>

        <section>
            <div class="{res.style.stripe} {res.style.light} {res.style.centered} {page.style.features}">
                <a href="#" class="{res.style.btn}">Contact us</a>
                <h2>Features</h2>
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                    <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full}">
                        <i class="{fonts.icons.iconCommRes}"></i>
                        <h3>Community
                            <span>Resources</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor.
                            Aenean massa.
                        </p>
                    </div>
                    <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full}">
                        <i class="{fonts.icons.iconRespTime}"></i>
                        <h3>Response
                            <span>Time</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor.
                            Aenean massa.
                        </p>
                    </div>
                    <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full}">
                        <i class="{fonts.icons.iconSupportCases}"></i>
                        <h3>Support
                            <span>Cases</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor.
                            Aenean massa.
                        </p>
                    </div>
                </div>
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                    <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_push_1_6} {res.grid.col_m_full}">
                        <i class="{fonts.icons.iconWebSupport}"></i>
                        <h3>Web
                            <span>Support</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor.
                            Aenean massa.
                        </p>
                    </div>
                    <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full}">
                        <i class="{fonts.icons.iconPhoneSuppport}"></i>
                        <h3>Phone
                            <span>Support</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor.
                            Aenean massa.
                        </p>
                    </div>
                </div>
            </div>
        </section>

    </g:HTMLPanel>
</ui:UiBinder>
