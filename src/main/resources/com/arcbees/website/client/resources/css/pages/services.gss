    @mixin animation_delay(2s);
    @mixin animation_duration(8s);
}

.learnAdapt i.icon4::before {
    @mixin rotate(-150deg);
}

.learnAdapt i.icon5 {
    margin-left: 12px;

    top: 81px;
    left: 50%;

    @mixin animation_delay(2.1s);
    @mixin animation_duration(8s);
}

.learnAdapt i.icon5::before {
    @mixin rotate(-90deg);
}

.learnAdapt i.icon6 {
    margin-left: -29px;

    top: 162px;
    left: 50%;

    @mixin animation_delay(2.2s);
    @mixin animation_duration(8s);
}

.learnAdapt i.icon6::before {
    @mixin rotate(-30deg);
}

@media (max-width: 649px) {
    .learnAdapt i.iconHexa {
        font-size: 4.25rem;

        margin-left: -21px;
    }
}

/* -- deliver -- */

.deliver {
    margin-bottom: 40px;
}

.deliver .diagram {
    margin: 30px 0 80px;
    height: 80px;

    position: relative;
}

.deliver i.iconHexa {
    color: #000;
    font-size: 5.25rem;

    margin-left: -26px;

    position: absolute;
    left: 50%;
    top: 20px;

    @mixin animation_duration(5s);
}

.deliver .bar1,
.deliver .bar2,
.deliver .bar3,
.deliver .bar4,
.deliver .bar5 {
    background-color: #000;

    left: 50%;
    position: absolute;

    @mixin rounded(5px);
    @mixin animation_name(deliver);
    @mixin animation_duration(5s);
}

.deliver .bar1 {
    margin-left: -2px;
    height: 17px;
    width: 4px;

    top: 0;

    @mixin animation_delay(.6s);
}

.deliver .bar2 {
    margin-left: -52px;
    width: 17px;
    height: 4px;

    top: 40px;

    @mixin animation_delay(.2s);
}

.deliver .bar3 {
    margin-left: -37px;
    width: 17px;
    height: 4px;

    top: 15px;

    @mixin rotate(45deg);
    @mixin animation_delay(.4s);
}

.deliver .bar4 {
    margin-left: 19px;
    width: 17px;
    height: 4px;

    top: 15px;

    @mixin rotate(-45deg);
    @mixin animation_delay(.8s);
}

.deliver .bar5 {
    margin-left: 34px;
    width: 17px;
    height: 4px;

    top: 40px;

    @mixin animation_delay(1s);
}

@keyframes deliver {
    from {opacity: 0;}
    to {opacity: 1;}
}

@-webkit-keyframes deliver {
    from {opacity: 0;}
    to {opacity: 1;}
}

@media (max-width: 649px) {
    .deliver i.iconHexa {
        font-size: 4.25rem;

        margin-left: -21px;

        top: 27px;
    }
}
