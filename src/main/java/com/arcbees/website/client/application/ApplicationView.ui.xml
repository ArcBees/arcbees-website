<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
        ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
        ui:generateLocales="en">

    <ui:with field="res" type="com.arcbees.website.client.resources.AppResources"/>
    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>

    <g:HTMLPanel>

        <!-- Main content -->
        <g:SimplePanel ui:field="main"/>

        <footer class="{res.style.footer}">
            <p>
                <i class="{fonts.icons.iconArcbees}"><span class="{res.style.hidden}">Arcbees</span></i>
                Architects of simplicity
            </p>
        </footer>

    </g:HTMLPanel>
</ui:UiBinder>
