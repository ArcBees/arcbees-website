<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
        ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
        ui:generateLocales="en">

    <ui:import field="com.arcbees.website.client.NameTokens.*"/>

    <ui:with field="doc" type="com.arcbees.website.client.resources.DocsResources"/>
    <ui:with field="res" type="com.arcbees.website.client.resources.AppResources"/>
    <ui:with field="page" type="com.arcbees.website.client.resources.DeveloppersResources"/>
    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>

    <g:HTMLPanel styleName="{page.style.developpers}">

        <!-- Docs content -->
        <g:SimplePanel ui:field="developpers"/>

    </g:HTMLPanel>
</ui:UiBinder>
