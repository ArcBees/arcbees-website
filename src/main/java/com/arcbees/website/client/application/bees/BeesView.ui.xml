                        <div class="{page.style.bee} {page.style.bee3}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{BEE_JULIEN}">
                                        <h3>Julien
                                            <span>Dramaix</span>
                                        </h3>
                                        <p><ui:msg description="Julien's title">Lead Software Engineer, Bee-r cool-er</ui:msg></p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee1}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{BEE_MANON}">
                                        <h3>Manon
                                            <span>Gruaz</span>
                                        </h3>
                                        <p><ui:msg description="Manon's title">Lead Designer, Branding goddess and Loving Bee</ui:msg></p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee2}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{BEE_JASON}">
                                        <h3>Jason
                                            <span>Lemay</span>
                                        </h3>
                                        <p><ui:msg description="Jason's title">Dual wielding Front-end Specialist</ui:msg></p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee3}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{BEE_MAXIME}">
                                        <h3>Maxime
                                            <span>Mériouma-Caron</span>
                                        </h3>
                                        <p><ui:msg description="Maxime's title">Night Shadow Developer</ui:msg></p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee1}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{BEE_CHRISTOPHER}">
                                        <h3>Christopher
                                            <span>Viel</span>
                                        </h3>
                                        <p><ui:msg description="Christopher title">Crazy Code Mastermind</ui:msg></p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee2}">
                            <div class="{page.style.beeHolder1}">
