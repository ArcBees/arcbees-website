<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="res" type="com.arcbees.website.client.resources.AppResources"/>
    <ui:with field="page" type="com.arcbees.website.client.resources.PageServicesResources"/>
    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>
    <ui:with field="animations" type="com.arcbees.website.client.resources.AnimationsResources"/>

    <ui:import field="com.arcbees.website.client.NameTokens.*"/>

    <g:HTMLPanel styleName="{page.style.services}">

        <h1 class="{fonts.icons.iconArcbees} {res.style.withLogo}">
            <span>Professional services</span>
        </h1>

        <section class="{res.style.stripe} {res.style.header} {page.style.header} {res.style.colored}">
            <div>
                <i class="{fonts.icons.iconArcbees} {page.style.iconPyramid} {page.style.pyramidAnim1} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.iconPyramid} {page.style.pyramidAnim2} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.iconPyramid} {page.style.pyramidAnim3} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.iconPyramid} {page.style.pyramidAnim4} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.iconPyramid} {page.style.pyramidAnim5} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.iconPyramid} {page.style.pyramidAnim6} {animations.style.animation}"></i>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.dark} {res.style.triangleBottom} {res.style.nextLight}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper} {res.style.focusBlock}">
                <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_push_1_4} {res.grid.col_m_full} {res.grid.col_m_push_0}">

                    <h2 class="{res.style.stripeTitle}">Professional
                        <span>services</span>
                    </h2>
                    <p>Create and build products
                        that optimise process, habits and thinking,
                        demonstrate value and simplicity,
                        and transform the way developers work around the world.
                    </p>

                </div>
            </div>
        </section>

        <section class="{page.style.realisations}">
            <div class="{res.style.stripe} {res.style.centered}">
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                    <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full} {page.style.knowledge}">
                        <div class="{page.style.icons}">
                            <i class="{fonts.icons.iconArcbees} {page.style.icon1}"></i>
                            <div class="{page.style.dots}">
                                <span class="{page.style.dot}"></span>
                                <span class="{page.style.dot}"></span>
                                <span class="{page.style.dot}"></span>
                            </div>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon2}"></i>
                        </div>

                        <h3>knowledge
                            <span>transfert</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor. Aenean massa. </p>
                    </div>
                    <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full}">
                        <i class="{fonts.icons.iconHive}"></i>
                        <h3>the bee
                            <span>way</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor. Aenean massa. </p>
                    </div>
                    <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full}">
                        <i class="{fonts.icons.iconPyramid}"></i>
                        <h3>work with
                            <span>the best</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor. Aenean massa. </p>
                    </div>
                </div>
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                    <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_m_full} {page.style.worksEverywhere}">
                        <div class="{page.style.icons}">
                            <i class="{fonts.icons.iconArcbees} {page.style.icon1}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon2}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon3}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon4}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon5}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon6}"></i>
                        </div>
                        <h3>works
                            <span>everywhere</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor. Aenean massa. </p>
                    </div>
                    <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_m_full} {page.style.beauty}">
                        <i class="{fonts.icons.iconArcbees}"></i>
                        <i class="{fonts.icons.iconhexaEmpty} {page.style.icon2}"></i>
                        <h3>beauty
                            <span>and function</span>
                        </h3>
                        <p>Lorem ipsum dolor sit amet, consectetuer adipiscing elit. Aenean commodo ligula eget dolor. Aenean massa. </p>
                    </div>
                </div>
            </div>
        </section>

        <section class="{page.style.trusted} {res.style.stripe} {res.style.medium2} {res.style.centered}">
            <div class="{res.style.wrapper}">
                <h2>Trusted by</h2>
                <ul>
                    <li>
                        <a href="#">
                            <g:Image resource="{page.workTrustedRoler}"/>
                        </a>
                    </li>
                    <li>
                        <a href="http://zafin.com/" target="_blank">
                            <g:Image resource="{page.workTrustedZafin}"/>
                        </a>
                    </li>
                    <li>
                        <a href="http://www.lillegroup.com/" target="_blank">
                            <g:Image resource="{page.workTrustedLille}"/>
                        </a>
                    </li>
                </ul>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.centered} {res.style.triangleBottom} {res.style.light} {res.style.nextColored} {page.style.beeWay}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col_1_2} {res.grid.col_push_1_4}">
                    <div class="{page.style.lightbulb}">
                        <h2>The
                            <span>bee way</span>
                        </h2>
                        <p>Agile is embedded in the ArcBees way of life. It drives our culture and productivity.</p>
                        <div class="{page.style.diagram}">
                            <div class="{page.style.light} {page.style.bar1} {animations.style.animation} {animations.style.infinite}"></div>
                            <div class="{page.style.light} {page.style.bar2} {animations.style.animation} {animations.style.infinite}"></div>
                            <div class="{page.style.light} {page.style.bar3} {animations.style.animation} {animations.style.infinite}"></div>
                            <div class="{page.style.light} {page.style.bar4} {animations.style.animation} {animations.style.infinite}"></div>
                            <div class="{page.style.light} {page.style.bar5} {animations.style.animation} {animations.style.infinite}"></div>
                            <g:Image resource="{page.servicesLightBulb}"/>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section class="{res.style.stripe} {res.style.colored} {res.style.centered} {page.style.howWeWork}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col_1_2} {res.grid.col_push_1_4}">
                    <div class="{page.style.partner}">
                        <h3>we work as partner</h3>
                        <p>Small teams working on big challenges in total partnership with our clients.</p>
                        <div class="{page.style.diagram}">
                            <i class="{fonts.icons.iconArcbees} {page.style.icon1} {animations.style.animation} {animations.style.infinite} {animations.style.bounceIn}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon2} {animations.style.animation} {animations.style.infinite} {animations.style.bounceIn}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon3} {animations.style.animation} {animations.style.infinite} {animations.style.bounceIn}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon4} {animations.style.animation} {animations.style.infinite} {animations.style.bounceIn}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon6} {animations.style.animation} {animations.style.infinite} {animations.style.bounceIn}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon5} {page.style.iconDark} {animations.style.animation} {animations.style.infinite} {animations.style.bounceIn}"></i>
                            <i class="{fonts.icons.iconhexagon} {page.style.iconHexa}"></i>
                        </div>
                    </div>
                    <div class="{page.style.iterative}">
                        <h3>A simple, iterative process</h3>
                        <p>The Agile remedy is to build a small product with just a few features and give it to real
                            users.
                        </p>
                        <div class="{page.style.diagram}">
                            <div class="{page.style.launch}">
                                <strong>Launch 1</strong>
                            </div>
                            <div class="{page.style.iteration1}">
                                <strong>Sprint 1</strong>
                            </div>
                            <div class="{page.style.iconHexa} {animations.style.animation} {animations.style.infinite} {animations.style.hexaMove}">
                                <i class="{fonts.icons.iconhexagon}"></i>
                            </div>

                            <div class="{page.style.circleQuarter}"></div>
                            <div class="{page.style.circle} {animations.style.animation} {animations.style.infinite} {animations.style.spinCCW}">
                                <div class="{page.style.bar1}"></div>
                                <div class="{page.style.bar2}"></div>
                                <div class="{page.style.bar3}"></div>
                                <div class="{page.style.bar4}"></div>
                            </div>
                        </div>
                    </div>
                    <div class="{page.style.codeReview}">
                        <h3>Code review for every line</h3>
                        <p>You build that as quickly and cheaply as possible, and watch people use it.
                        </p>
                        <div class="{page.style.diagram}">
                            <div class="{page.style.bar1}"></div>
                            <div class="{page.style.bracket1}"> &lt; </div>
                            <div class="{page.style.bracket2}"> &gt; </div>
                            <i class="{fonts.icons.iconhexagon} {page.style.iconHexa} {animations.style.animation} {page.style.hexaMove}"></i>
                            <div class="{animations.style.animation} {animations.style.infinite} {animations.style.spin} {page.style.gear1}">
                                <i class="{fonts.icons.iconGearing} "></i>
                            </div>

                            <div class="{animations.style.animation} {animations.style.infinite} {animations.style.spinCCW} {page.style.gear2}">
                                <i class="{fonts.icons.iconGearing}"></i>
                            </div>

                            <div class="{animations.style.animation} {animations.style.infinite} {animations.style.spin} {page.style.gear3}">
                                <i class="{fonts.icons.iconGearing}"></i>
                            </div>

                        </div>
                    </div>
                    <div class="{page.style.costRisk}">
                        <h3>Manage cost and limit risk</h3>
                        <p>You build that as quickly and cheaply as possible, and watch people use it.</p>
                        <div class="{page.style.diagram}">
                            <i class="{fonts.icons.iconhexagon} {page.style.icon1} {animations.style.animation} {animations.style.line}"></i>
                            <i class="{fonts.icons.iconhexagon} {page.style.icon2} {animations.style.animation} {animations.style.line}"></i>
                            <i class="{fonts.icons.iconhexagon} {page.style.icon3} {animations.style.animation} {animations.style.line}"></i>
                            <i class="{fonts.icons.iconhexagon} {page.style.icon4} {animations.style.animation} {animations.style.line}"></i>
                            <i class="{fonts.icons.iconhexagon} {page.style.icon5} {animations.style.animation} {animations.style.line}"></i>
                            <i class="{fonts.icons.iconhexagon} {page.style.icon6} {animations.style.animation} {animations.style.line}"></i>
                            <div class="{page.style.bar1} {animations.style.animation} {animations.style.line}"></div>
                            <div class="{page.style.bar2} {animations.style.animation} {animations.style.line}"></div>
                            <div class="{page.style.bar3} {animations.style.animation} {animations.style.line}"></div>
                            <div class="{page.style.bar4} {animations.style.animation} {animations.style.line}"></div>
                            <div class="{page.style.bar5} {animations.style.animation} {animations.style.line}"></div>
                            <div class="{page.style.bar6} {animations.style.animation} {animations.style.line}"></div>
                            <div class="{page.style.bar7} {animations.style.animation} {animations.style.line}"></div>
                        </div>
                    </div>
                    <div class="{page.style.learnAdapt}">
                        <h3>Learn and adapt</h3>
                        <p>Then you improve the product based on what you learn.</p>
                        <div class="{page.style.diagram}">
                            <div class="{page.style.bar1} {animations.style.animation}"></div>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon1} {animations.style.animation} {animations.style.infinite} {animations.style.leave}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon2} {animations.style.animation} {animations.style.infinite} {animations.style.leave2}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon3} {animations.style.animation} {animations.style.infinite} {animations.style.leave3}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon4} {animations.style.animation} {animations.style.infinite} {animations.style.leave4}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon5} {animations.style.animation} {animations.style.infinite} {animations.style.leave5}"></i>
                            <i class="{fonts.icons.iconArcbees} {page.style.icon6} {animations.style.animation} {animations.style.infinite} {animations.style.leave6}"></i>
                            <i class="{fonts.icons.iconhexagon} {page.style.iconHexa} {animations.style.animation} {animations.style.bounceIn}"></i>
                        </div>
                    </div>
                    <section class="{page.style.deliver}">
                        <h3>deliver the best product ever</h3>
                        <p>After many cycles of real-time feedback, the end product is well-tested and useful.</p>
                        <div class="{page.style.diagram}">
                            <div class="{page.style.bar1} {animations.style.animation} {animations.style.infinite}"></div>
                            <div class="{page.style.bar2} {animations.style.animation} {animations.style.infinite}"></div>
                            <div class="{page.style.bar3} {animations.style.animation} {animations.style.infinite}"></div>
                            <div class="{page.style.bar4} {animations.style.animation} {animations.style.infinite}"></div>
                            <div class="{page.style.bar5} {animations.style.animation} {animations.style.infinite}"></div>
                            <i class="{fonts.icons.iconhexagon} {page.style.iconHexa} {animations.style.animation} {animations.style.infinite} {animations.style.bounceIn}"></i>
                        </div>
                    </section>
                </div>
            </div>
        </section>

    </g:HTMLPanel>
</ui:UiBinder>
