<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
        ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
        ui:generateLocales="en">

    <ui:import field="com.arcbees.website.client.NameTokens.*"/>

    <ui:with field="doc" type="com.arcbees.website.client.resources.DocsResources"/>

    <g:HTMLPanel styleName="{doc.style.doc}">
        <!-- Docs content -->
        <g:SimplePanel ui:field="docs"/>
        <section class="{doc.style.support}">
            <h2>Need support?</h2>
            <a href="#{LICENSE}" class="{doc.style.btn}">Arcbees offer a variety of supports options</a>
            <div class="{doc.style.backTop}">
                <span data-icon="&#xe602;"></span>
            </div>
        </section>
    </g:HTMLPanel>
</ui:UiBinder>
