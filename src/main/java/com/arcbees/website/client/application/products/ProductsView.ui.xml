<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
        ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
        ui:generateLocales="en">

    <ui:import field="com.arcbees.website.client.NameTokens.*"/>

    <ui:with field="doc" type="com.arcbees.website.client.resources.DocsResources"/>
    <ui:with field="res" type="com.arcbees.website.client.resources.AppResources"/>
    <ui:with field="page" type="com.arcbees.website.client.resources.PageProductResources"/>
    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>

    <g:HTMLPanel styleName="{page.style.products}">

        <section class="{res.style.stripe} {res.style.centered} {page.style.productHead}" ui:field="productHead">
            <nav class="{page.style.menuHeader}">
                <ul>
                    <li>
                        <a href="#{GAE}" class="{fonts.icons.iconGae}" data-resource="gae">
                            <span class="{res.style.hidden}">Gae</span>
                        </a>
                    </li>
                    <li>
                        <a href="#{GWTP}" class="{fonts.icons.iconGwtp}" data-resource="gwtp">
                            <span class="{res.style.hidden}">GWTP</span>
                        </a>
                    </li>
                    <li>
                        <a href="#{JUKITO}" class="{fonts.icons.iconJukito}" data-resource="jukito">
                            <span class="{res.style.hidden}">Jukito</span>
                        </a>
                    </li>
                </ul>
            </nav>
            <div class="{page.style.arrow}"></div>
        </section>

        <!-- Docs content -->
        <g:SimplePanel ui:field="products"/>

        <section class="{res.style.stripe} {page.style.stripeSupport} {res.style.centered}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_full}">

                    <h3>Need support?</h3>
                    <a href="#" class="{res.style.btn}">Arcbees offer a variety of supports options</a>
                    <a href="#" data-icon="&#xe602;" class="{res.style.backTop}"></a>

                </div>
            </div>
        </section>
    </g:HTMLPanel>
</ui:UiBinder>
