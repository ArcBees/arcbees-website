@require "colors";
@require "geometria";
@require "gsss-mixins";
@require "gsss-animation";

@def WORK_BANNER resourceUrl("workBanner");

/* -- Page : services ---------------------------------------- */

.services {
    background: C_STRIPE_LIGHT;
}

/* Header animations */
/* ------------------------------ */

.services .header > div {
    margin: -227px 0 0 -267px;
    height: 455px;
    width: 534px;

    position: absolute;
    bottom: 0;
    left: 50%;
}

.header i.iconPyramid {
    color: C_TEXT;
    font-size: 21rem;

    right: 50%;
    position: absolute;
}

.pyramidAnim1,
.pyramidAnim2,
.pyramidAnim3 {
    bottom: -21px;

    @mixin animation_delay(.2s);
    @mixin animation_duration(.5s);
    @mixin animation_name(pyramid1);
}

.pyramidAnim1 {
    margin-right: 105px;
}

.pyramidAnim2 {
    margin-right: -95px;
}

.pyramidAnim3 {
    margin-right: -295px;
}

.pyramidAnim4,
.pyramidAnim5 {
    bottom: 154px;

    @mixin animation_delay(.6s);
    @mixin animation_duration(.5s);
    @mixin animation_name(pyramid2);
}

.pyramidAnim4 {
    margin-right: 5px;
}

.pyramidAnim5 {
    margin-right: -195px;
}

.pyramidAnim6 {
    margin-right: -95px;

    bottom: 329px;

    @mixin animation_delay(1s);
    @mixin animation_duration(.5s);
    @mixin animation_name(pyramid3);
}

@keyframes pyramid1 {
    0% {
        @mixin translate(0px, 240px);
        opacity: 0;
    }
    100% {
        @mixin translate(0px, 0px);
        opacity: 1;
    }
}

@-webkit-keyframes pyramid1 {
    0% {
        -webkit-transform: translate(0px, 240px);
        opacity: 0;
    }
    100% {
        -webkit-transform: translate(0px, 0px);
        opacity: 1;
    }
}

@keyframes pyramid2 {
    0% {
        @mixin translate(0px, 320px);
        opacity: 0;
    }
    100% {
        @mixin translate(0px, 0px);
        opacity: 1;
    }
}

@-webkit-keyframes pyramid2 {
    0% {
        -webkit-transform: translate(0px, 320px);
        opacity: 0;
    }
    100% {
        -webkit-transform: translate(0px, 0px);
        opacity: 1;
    }
}

@keyframes pyramid3 {
    0% {
        @mixin translate(0px, 500px);
        opacity: 0;
    }
    100% {
        @mixin translate(0px, 0px);
        opacity: 1;
    }
}

@-webkit-keyframes pyramid3 {
    0% {
        -webkit-transform: translate(0px, 500px);
        opacity: 0;
    }
    100% {
        -webkit-transform: translate(0px, 0px);
        opacity: 1;
    }
}

@media (max-width: 649px) {
    .header {
        -webkit-background-size: 90% auto;
        background-size: 90% auto;

        min-height: 250px;
    }

    .header i.iconPyramid {
        font-size: 11.5rem;
    }

    .pyramidAnim1,
    .pyramidAnim2,
    .pyramidAnim3 {
        bottom: -14px;

    @mixin animation_delay(.2s);
    @mixin animation_duration(.5s);
    @mixin animation_name(pyramid1);
    }

    .pyramidAnim1 {
        margin-right: 50px;
    }

    .pyramidAnim2 {
        margin-right: -57px;
    }

    .pyramidAnim3 {
        margin-right: -164px;
    }

    .pyramidAnim4,
    .pyramidAnim5 {
        bottom: 80px;

    @mixin animation_delay(.6s);
    @mixin animation_duration(.5s);
    @mixin animation_name(pyramid2);
    }

    .pyramidAnim4 {
        margin-right: -3px;
    }

    .pyramidAnim5 {
        margin-right: -111px;
    }

    .pyramidAnim6 {
        margin-right: -57px;

        bottom: 174px;

    @mixin animation_delay(1s);
    @mixin animation_duration(.5s);
    @mixin animation_name(pyramid3);
    }
}

/* advantages */
/* ------------------------------ */

.advantages h2 {
