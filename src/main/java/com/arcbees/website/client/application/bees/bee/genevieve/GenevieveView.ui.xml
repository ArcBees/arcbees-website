    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>

    <g:HTMLPanel>

        <seo:SeoWidget>
            <seo:Title>
                <ui:msg description="SEO - Bees - Geneviève - title">Geneviève Doyle - Arcbees</ui:msg>
            </seo:Title>
            <seo:Description>
                <ui:msg description="SEO - Bees - Geneviève - description">
                    Attorney
                </ui:msg>
