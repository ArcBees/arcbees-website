<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:ahive_navbar='urn:import:com.arcbees.hive.client.application.common.navbar'>
    <ui:with field='res' type='com.arcbees.hive.client.resource.Resources'/>
    <ui:style>
        .conteneurPrincipal {
            margin-left: auto;
            margin-right: auto;
            width: 950px;
            text-align: left;
        }
    </ui:style>
    <g:HTMLPanel styleName="{style.conteneurPrincipal}">
        <g:HTMLPanel ui:field="header"/>
        <ahive_navbar:NavbarView ui:field="navbarView"/>
        <g:HTMLPanel ui:field="mainContent" addStyleNames="{res.style.mainDegradeBgSprite}">
            <g:HTMLPanel ui:field="mainContent1" visible="false"/>
            <g:HTMLPanel ui:field="mainContent2" visible="false"/>
        </g:HTMLPanel>
        <g:SimplePanel ui:field="customers"/>
        <g:HTMLPanel ui:field="footer"/>
    </g:HTMLPanel>
</ui:UiBinder>
