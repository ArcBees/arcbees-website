<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:seo="urn:import:com.arcbees.seo.widget"
        xmlns:aw="urn:import:com.arcbees.website.client.application.ui"
        ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
        ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
        ui:generateLocales="default">

    <ui:with field="nameTokens" type="com.arcbees.website.client.NameTokensConstants"/>
    <ui:with field="res" type="com.arcbees.website.client.resources.AppResources"/>
    <ui:with field="page" type="com.arcbees.website.client.resources.PageExpertisesResources"/>
    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>
    <ui:with field="animations" type="com.arcbees.website.client.resources.AnimationsResources"/>

    <g:HTMLPanel styleName="{page.style.expertises}">

        <seo:SeoWidget>
            <seo:Title>
                <ui:msg description="SEO - Expertise - title">Expertise - Arcbees</ui:msg>
            </seo:Title>
            <seo:Description>
                <ui:msg description="SEO - Expertise - description">
                    We boost your productivity and improve your applications with our cutting edge services, customized
                    support and training.
                </ui:msg>
            </seo:Description>
        </seo:SeoWidget>

        <h1 class="{res.style.breadcrumb} {res.style.withLogo}">
            <ui:msg description="Expertise - page heading">Expertise</ui:msg>
        </h1>

        <section class="{res.style.stripe} {res.style.header} {page.style.header}">
            <div>
                <i class="{fonts.icons.iconArcbees} {page.style.icon1} {page.style.codeAnim1} {animations.style.animation}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.icon2} {page.style.codeAnim2} {animations.style.animation}"></i>
                <div class="{page.style.icon3} {page.style.codeAnim3} {animations.style.animation} {animations.style.bounceIn}">
                    <g:Image resource="{page.expertisesSlash}"/>
                </div>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.dark} {res.style.triangleBottom} {res.style.nextLight}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper} {res.style.focusBlock}">
                <div class="{res.grid.col} {res.grid.col_8} {res.grid.col_push_1_6} {res.grid.col_m_full} {res.grid.col_m_push_0}">
                    <h2 class="{res.style.stripeTitle}">
                        <ui:msg description="Expertise - main - title">Expertise</ui:msg>
                    </h2>
                    <p>
                        <ui:msg description="Expertise - main - text">Let our experts boost your productivity and
                            improve your applications with our cutting edge services, customized support and training.
                        </ui:msg>
                    </p>
                </div>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.centered} {page.style.services}">
            <div class="{res.style.wrapper} {res.grid.row} {res.style.clearfix}">
                <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_push_1_12} {res.grid.col_t_1_2} {res.grid.col_t_push_0} {res.grid.col_m_full} {res.grid.col_m_push_0}">
                    <h3>
                        <ui:msg description="Expertise - Services - title">Services</ui:msg>
                    </h3>
                    <p>
                        <ui:msg description="Expertise - Services - text">We can help transform your
                            business applications and improve your development practices.
                        </ui:msg>
                    </p>
                    <a href="#{nameTokens.SERVICES}" class="{res.style.btn}">
                        <ui:msg description="Btn - Learn more">Learn more</ui:msg>
                    </a>
                </div>
                <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_push_1_6} {res.grid.col_t_1_2} {res.grid.col_t_push_0} {res.grid.col_m_full} {res.grid.col_m_push_0}">
                    <h3>
                        <ui:msg description="Expertise - Support - title">Support</ui:msg>
                    </h3>
                    <p>
                        <ui:msg description="Expertise - Support - text">Any time you have a concern to technical
                            challenge using our products, you can rely on our experts to you find solutions.
                        </ui:msg>
                    </p>
                    <a href="#{nameTokens.SUPPORT}" class="{res.style.btn}">
                        <ui:msg description="Btn - Learn more">Learn more</ui:msg>
                    </a>
                </div>
            </div>
            <div class="{res.style.wrapper} {res.grid.row} {res.style.clearfix}">
                <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_push_1_3} {res.grid.col_t_1_2} {res.grid.col_t_push_3} {res.grid.col_m_full} {res.grid.col_m_push_0}">
                    <h3>
                        <ui:msg description="Expertise - Training - title">Training</ui:msg>
                    </h3>
                    <p>
                        <ui:msg description="Expertise - Training - text">Want to learn how to use our products faster and
                            better? Let our experts compile and deliver customized training just for you.
                        </ui:msg>
                    </p>
                </div>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.colored} {res.style.centered} {page.style.tools}">
            <div class="{res.style.wrapper} {res.grid.row} {res.style.clearfix}">
                <div class="{page.style.plus}">+</div>
                <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full}">
                    <i class="{fonts.icons.iconHtml5}"></i>
                </div>
                <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full}">
                    <i class="{fonts.icons.iconGwt}"></i>
                </div>
                <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_m_full}">
                    <i class="{fonts.icons.iconGCloudPlatform}"></i>
                </div>
                <div class="{page.style.equal}">=</div>
            </div>
        </section>

        <aw:ProductsCarousel/>
    </g:HTMLPanel>
</ui:UiBinder>
