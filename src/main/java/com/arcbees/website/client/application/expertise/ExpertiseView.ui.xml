    <ui:with field="res" type="com.arcbees.website.client.resources.AppResources"/>
    <ui:with field="page" type="com.arcbees.website.client.resources.PageExpertisesResources"/>
    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>
    <ui:with field="animations" type="com.arcbees.website.client.resources.AnimationsResources"/>

    <g:HTMLPanel styleName="{page.style.expertises}">

        <h1 class="{fonts.icons.iconArcbees} {res.style.withLogo}"><span>Expertises</span></h1>

        <section class="{res.style.stripe} {page.style.headerBg}">
        </section>

        <section class="{res.style.stripe} {res.style.colored}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_m_full}">
                    <h2 class="{res.style.slogan}">Bee
                        <span>the
                            <strong>code</strong>
                        </span>
                    </h2>
                </div>
            </div>
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_push_1_2} {res.grid.col_m_full} {res.grid.col_m_push_0}">
                    <p>ArcBees offers consulting and development services that can help you improve the quality of your
                        web applications and the performance of your development team during your project.
                    </p>
                    <p>
                        <strong>A qualified member of ArcBees will join your team</strong>
                        to help you with your specific need, be it web
                        development, application design, tooling, or process and methodology. Our consultant will be
                        easily accessible either physically or by telecommunication.
                    </p>
                </div>
            </div>
        </section>

        <section class="{page.style.whatWeDo}">
            <div class="{res.style.stripe} {res.style.colored} {res.style.centered}">
                <h2>what we do</h2>
            </div>
            <div class="{res.style.stripe} {res.style.centered}">
                <div class="{res.style.wrapper} {res.grid.row} {res.style.clearfix}">
                    <div class="{res.grid.col} {res.grid.col_full}">
                        <h3 class="{res.style.stripeTitle}">Consulting</h3>
                        <a href="#" class="{res.style.btn}">How we help you improve your performance</a>
                    </div>
                </div>
            </div>
            <div class="{res.style.stripe} {res.style.dark} {page.style.developping}">
                <div class="{res.style.wrapper} {res.grid.row} {res.style.clearfix}">
                    <div class="{res.grid.col} {res.grid.col_full} {res.style.centered}">
                        <h3 class="{res.style.stripeTitle}">Developping</h3>
                        <a href="#" class="{res.style.btn}">How we provide you a key-in-hand development team</a>
                    </div>
                    <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_m_full}">
