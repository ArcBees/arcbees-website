<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
        ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
        ui:generateLocales="en">

    <ui:import field="com.arcbees.website.client.NameTokens.*"/>

    <ui:with field="doc" type="com.arcbees.website.client.resources.DocsResources"/>
    <ui:with field="res" type="com.arcbees.website.client.resources.AppResources"/>
    <ui:with field="page" type="com.arcbees.website.client.resources.DevelopersResources"/>
    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>

    <g:HTMLPanel styleName="{page.style.developers}">

        <section class="{res.style.stripe} {res.style.header} {page.style.header} {res.style.colored}">
            <div class="{res.style.centered}">
                <p>Welcome to arcbees
                    <span>platform for developers</span>
                </p>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.light}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_1_2}">
                    <h2 class="{res.style.slogan}">Bee
                        <span>your
                            <strong>best</strong>
                        </span>
                    </h2>
                </div>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.light} {res.style.centered} {page.style.reason}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_1_3}">
                    <i class="{fonts.icons.iconSignCode} {page.style.simplifyCode}"></i>
                    <h3>Simplify your code</h3>
                </div>
                <div class="{res.grid.col} {res.grid.col_1_3}">
                    <i class="{fonts.icons.iconDevTime}"></i>
                    <h3>Optimize your time</h3>
                </div>
                <div class="{res.grid.col} {res.grid.col_1_3}">
                    <i class="{fonts.icons.iconUser}"/>
                    <h3>Enhance user experience</h3>
                </div>
            </div>
        </section>

        <section class="{res.style.centered}">
            <div class="{res.style.stripe}">
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                    <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_push_1_4} {res.grid.col_m_full} {res.grid.col_m_push_0}">
                        <h2>That's the reason <span>we create these products</span></h2>
                    </div>
                </div>
            </div>

            <div class="{res.style.stripe} {res.style.medium2} {page.style.productSupport}">
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">

                    <div class="{res.grid.col} {res.grid.col_1_3} {page.style.gwtp}">
                        <h3>
                            <i class="{fonts.icons.iconGwtp}"></i>
                            GWTP
                        </h3>
                        <p>Combining powerful features, GWTP is an out-of-the-box solution that supports all the
                            requirements of modern web apps.
                        </p>
                        <p>
                            <a href="#/developers/gwtp" class="{res.style.btn}">Learn more</a>
                        </p>
                        <p>
                            <a href="#" class="{page.style.docsLink}">+ Docs</a>
                        </p>
                    </div>

                    <div class="{res.grid.col} {res.grid.col_1_3} {page.style.jukito}">
                        <h3>
                            <i class="{fonts.icons.iconJukito}"></i>
                            Jukito
                        </h3>
                        <p>Jukito lets you use the power of dependency injection in all your tests. Jukito combine power
                            of
                            JUnit, Guice and Mockito.
                        </p>
                        <p>
                            <a href="#/developers/jukito" class="{res.style.btn}">Learn more</a>
                        </p>
                        <p>
                            <a href="#" class="{page.style.docsLink}">+ Docs</a>
                        </p>
                    </div>

                    <div class="{res.grid.col} {res.grid.col_1_3} {page.style.chosen}">
                        <h3>
                            <i class="{fonts.icons.iconChosen}"></i>
                            GWT
                            <span>Chosen</span>
                        </h3>
                        <p>GWT Chosen is a javascript plugin (for jQuery and Prototype) that makes long, unwieldy select
                            boxes much more user-friendly.
                        </p>
                        <p>
                            <a href="#/developers/chosen" class="{res.style.btn}">Learn more</a>
                        </p>
                        <p>
                            <a href="#" class="{page.style.docsLink}">+ Docs</a>
                        </p>
                    </div>

                </div>
            </div>
        </section>

        <section class="{res.style.centered}">
            <div class="{res.style.stripe} {res.style.medium2}">
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                    <div class="{res.grid.col} {res.grid.col_full}">
                        <h2>And more</h2>
                    </div>
                </div>
            </div>

            <div class="{res.style.stripe} {res.style.centered} {page.style.moreSupport}">
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">

                    <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_m_full}">
                        <h3>Gss on Steroids</h3>
                        <p>
                            <a href="#" class="{res.style.btn}">Docs</a>
                        </p>
                    </div>

                    <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_m_full}">
                        <h3>GWT Elastic</h3>
                        <p>
                            <a href="#" class="{res.style.btn}">Docs</a>
                        </p>
                    </div>

                    <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_m_full}">
                        <h3>GWT Stripe</h3>
                        <p>
                            <a href="#" class="{res.style.btn}">Docs</a>
                        </p>
                    </div>

                    <div class="{res.grid.col} {res.grid.col_1_2} {res.grid.col_m_full}">
                        <h3>GWT Tooltip</h3>
                        <p>
                            <a href="#" class="{res.style.btn}">Docs</a>
                        </p>
                    </div>

                </div>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.light} {res.style.centered}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_full}">
                    <h2>Who are we?</h2>
                    <a href="#{SUPPORT}" class="{res.style.btn}">Go to our website</a>
                </div>
            </div>
        </section>

    </g:HTMLPanel>
</ui:UiBinder>
