/* -- partner -- */

.partner {
    margin-bottom: 180px;
}

.partner .diagram {
    height: 333px;
    margin-top: 75px;

    position: relative;
}

.partner .diagram::before,
.partner .diagram::after {
    content: "";
    background-color: #000;

    display: block;
    height: 57px;
    width: 5px;
    margin-top: -51px;
    margin-left: -2.5px;

    position: absolute;
    left: 50%;

    @mixin rounded(5px);
}

.partner .diagram::before {
    top: 0px;
}

.partner .diagram::after {
    height: 108px;
    margin-bottom: -115px;

    bottom: 0;
}

.partner i {
    font-size: 15.5rem;
    color: C_TEXT_LIGHT;

    position: absolute;
}

.partner i.icon1 {
    margin-right: -39px;

    top: 133px;
    right: 50%;

    @mixin animation_delay(.5s);
    @mixin animation_duration(5s);
}

.partner i.icon1::before {
    @mixin rotate(30deg);
}

.partner i.icon2 {
    top: 67px;
    right: 50%;

    @mixin animation_delay(.8s);
    @mixin animation_duration(5s);
}

.partner i.icon2::before {
    @mixin rotate(90deg);
}

.partner i.icon3 {
    margin-right: -38px;

    top: 0px;
    right: 50%;

    @mixin animation_delay(.9s);
    @mixin animation_duration(5s);
}

.partner i.icon3::before {
    @mixin rotate(150deg);
}

.partner i.icon4 {
    margin-left: -39px;

    top: -1px;
    left: 50%;

    @mixin animation_delay(1s);
    @mixin animation_duration(5s);
}

.partner i.icon4::before {
    @mixin rotate(-150deg);
}

.partner i.icon5 {
    top: 66px;
    left: 50%;

    @mixin animation_delay(1.1s);
    @mixin animation_duration(5s);
}

.partner i.icon5::before {
    @mixin rotate(-90deg);
}

.partner i.icon5.iconDark {
    color: #000;

    top: 66px;
    left: 50%;

    @mixin animation_name(partner);
}

.partner i.icon6 {
    margin-left: -38px;

    top: 133px;
    left: 50%;

    @mixin animation_delay(1.2s);
    @mixin animation_duration(5s);
}

.partner i.icon6::before {
    @mixin rotate(-30deg);
}

.partner i.iconHexa {
    color: #000;
    font-size: 3rem;

    margin-left: -15px;

    left: 50%;
    top: 152px;
}

@keyframes partner {
    from {
        @mixin translate(100px, 0px);
        opacity:0;
    }
    to {
        @mixin translate(0px, 0px);
        opacity: 1;
    }
}

@-webkit-keyframes partner {
    from {
        -webkit-transform: translate(100px,0px);
        opacity:0;
    }
    to {
        -webkit-transform: translate(0px,0px);
        opacity: 1;
    }
}

@media (max-width: 649px) {
    .partner .diagram::before,
    .partner .diagram::after {
        margin-top: -50px;
    }

    .partner .diagram::after {
        margin-bottom: -95px;
    }

    .partner i.iconHexa {
        top: 143px;
    }

    .partner i {
        font-size: 12.5rem;
    }

    .partner i.icon1 {
        margin-right: -28px;

        top: 143px;
    }

    .partner i.icon2 {
