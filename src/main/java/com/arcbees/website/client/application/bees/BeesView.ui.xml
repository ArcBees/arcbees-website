<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="default">

    <ui:with field="nameTokens" type="com.arcbees.website.client.NameTokensConstants"/>
    <ui:with field="res" type="com.arcbees.website.client.resources.AppResources"/>
    <ui:with field="page" type="com.arcbees.website.client.resources.PageBeesResources"/>
    <ui:with field="fonts" type="com.arcbees.website.client.resources.FontResources"/>
    <ui:with field="animations" type="com.arcbees.website.client.resources.AnimationsResources"/>

    <g:HTMLPanel styleName="{page.style.bees}">

        <h1 class="{fonts.icons.iconArcbees} {res.style.withLogo}">
            <span>Bees</span>
        </h1>

        <section class="{res.style.stripe} {res.style.header} {page.style.header} {res.style.light} {page.style.intro}">
            <div>
                <i class="{fonts.icons.iconArcbees} {page.style.icon1} {animations.style.animation} {animations.style.bounceIn}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.icon2} {animations.style.animation} {animations.style.bounceIn}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.icon3} {animations.style.animation} {animations.style.bounceIn}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.icon4} {animations.style.animation} {animations.style.bounceIn}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.icon5} {animations.style.animation} {animations.style.bounceIn}"></i>
                <i class="{fonts.icons.iconArcbees} {page.style.icon6} {animations.style.animation} {animations.style.bounceIn}"></i>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.dark} {res.style.triangleBottom} {res.style.nextColored}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper} {res.style.focusBlock}">
                <div class="{res.grid.col} {res.grid.col_8} {res.grid.col_push_1_6} {res.grid.col_m_full} {res.grid.col_m_push_0}">

                    <h2 class="{res.style.stripeTitle}">
                        <ui:msg description="Our Culture">Our Culture</ui:msg>
                    </h2>
                    <p>
                        <ui:msg description="Our Culture - Paragraph -">Every Bee is entitled to their own opinion and
                            to challenge the Queenbee, but we are a hive, a team, with shared values and purpose.
                        </ui:msg>
                    </p>

                </div>
            </div>
        </section>

        <section
                class="{res.style.stripe} {res.style.colored} {res.style.triangleBottom} {res.style.nextLight}  {res.style.centered}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_1_3} {res.grid.col_push_1_3} {res.grid.col_m_full} {res.grid.col_m_push_0}">
                    <div class="{page.style.slides}">
                        <div class="{page.style.slide}">
                            <h2>
                                <ui:msg description="Bee dedicated">Bee Dedicated</ui:msg>
                            </h2>
                            <p>
                                <ui:msg description="Bee dedicated - Paragraph -">When we agree to take on a client’s
                                    project, it’s not just their project anymore. It’s ours too, and there’s nothing we
                                    won't do to make it a success.
                                </ui:msg>
                            </p>
                        </div>
                    </div>
                </div>
                <a href="#" class="{page.style.prev}">
                    <span class="{res.style.hidden}">Prev</span>
                </a>
                <a href="#" class="{page.style.next}">
                    <span class="{res.style.hidden}">Next</span>
                </a>
                <nav class="{page.style.pager} {res.style.centered}">
                    <ul>
                        <li>
                            <a href="#" class="{page.style.active}"></a>
                        </li>
                        <li>
                            <a href="#"></a>
                        </li>
                        <li>
                            <a href="#"></a>
                        </li>
                        <li>
                            <a href="#"></a>
                        </li>
                        <li>
                            <a href="#"></a>
                        </li>
                    </ul>
                </nav>
            </div>
        </section>

        <section ui:field="bees" class="{res.style.stripe} {res.style.light} {res.style.centered}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_full}">

                    <h2>
                        <ui:msg description="Our Bees">Our Bees</ui:msg>
                    </h2>

                    <div class="{page.style.hive}">
                        <div class="{page.style.bee} {page.style.bee1}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_CHRISTIAN}">
                                        <h3>Christian Goudreau</h3>
                                        <p>
                                            <ui:msg description="Christian's title">BEE-EO and co-founder</ui:msg>
                                        </p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee2}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_FRANCOIS}">
                                        <h3>François Dion</h3>
                                        <p>
                                            <ui:msg description="Francois's title">Strategic advisor, associate and
                                                Board chairman
                                            </ui:msg>
                                        </p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee3}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_PHILIPPEB}">
                                        <h3>Philippe Beaudoin</h3>
                                        <p>Advisor and co-founder</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee1}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_PHILIPPEA}">
                                        <h3>Philippe Araujo</h3>
                                        <p>Business Knight</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee2}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_GENEVIEVE}">
                                        <h3>Geneviève Doyle</h3>
                                        <p>Attorney</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee3}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_JULIEN}">
                                        <h3>Julien Dramaix</h3>
                                        <p>Lead Software Engineer, Bee-r cool-er</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee1}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_MANON}">
                                        <h3>Manon Gruaz</h3>
                                        <p>Lead Designer, Branding goddess and Loving Bee</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee2}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_JASON}">
                                        <h3>Jason Lemay</h3>
                                        <p>Dual wielding Front-end Specialist</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee3}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_MAXIME}">
                                        <h3>Maxime Mériouma-Caron</h3>
                                        <p>Night Shadow Developer</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee1}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_CHRISTOPHER}">
                                        <h3>Christopher Viel</h3>
                                        <p>Crazy Code Mastermind</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee2}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_SIMONPIERRE}">
                                        <h3>Simon-Pierre Gingras</h3>
                                        <p>
                                            <ui:msg description="Simon-Pierre's title">Developer of things, Chief Bacon Evangelist</ui:msg>
                                        </p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee3}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_JOEL}">
                                        <h3>Joel Trottier-Hébert</h3>
                                        <p>Bugs writer</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee1}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_LARRY}">
                                        <h3>Larry Matte</h3>
                                        <p>Undercover Front-end Agent</p>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div class="{page.style.bee} {page.style.bee2}">
                            <div class="{page.style.beeHolder1}">
                                <div class="{page.style.beeHolder2}">
                                    <g:Image resource="{page.beeChristian}"></g:Image>
                                    <a href="#{nameTokens.BEE_JEANCHRISTOPHE}">
                                        <h3>Jean-Christophe Lariviere</h3>
                                        <p>Coder in the North</p>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </section>

        <section class="{res.style.stripe} {res.style.light} {res.style.centered}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_full}">
                    <h2>Wanna bee?</h2>
                </div>
            </div>
        </section>

        <g:HTMLPanel tag="section" ui:field="quizSection" styleName="{res.style.stripe} {res.style.dark} {res.style.centered}">
            <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                <div class="{res.grid.col} {res.grid.col_full}">

                    <h2 ui:field="quizTitle">
                        <ui:msg description="Bee-attitude">Bee-attitude</ui:msg>
                    </h2>

                    <p>
                        <g:Button ui:field="initQuizButton" styleName="{res.style.btn}">Go, I'm ready to beeez</g:Button>
                    </p>

                </div>
            </div>
        </g:HTMLPanel>

        <section class="{page.style.offers} {res.style.stripe} {res.style.light} {res.style.centered}">
            <h2>
                <ui:msg description="Spontaneous applications">Spontaneous applications</ui:msg>
            </h2>
            <div class="{res.style.stripe}">
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                    <div class="{res.grid.col} {res.grid.col_full}">
                        <h3>Software engineer</h3>
                        <a href="#" class="{res.style.btn}">How can you be the best programmer you can be</a>
                    </div>
                </div>
            </div>
            <div class="{res.style.stripe}">
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                    <div class="{res.grid.col} {res.grid.col_full}">
                        <h3>Web developer</h3>
                        <a href="#" class="{res.style.btn}">Awesome CSS Master</a>
                    </div>
                </div>
            </div>
            <div class="{res.style.stripe}">
                <div class="{res.grid.row} {res.style.clearfix} {res.style.wrapper}">
                    <div class="{res.grid.col} {res.grid.col_full}">
                        <h3>Designer slave</h3>
                        <a href="#" class="{res.style.btn}">We hope you like to suffer</a>
                    </div>
                </div>
            </div>
        </section>

        <div class="{page.style.popup}" ui:field="popup">
            <div class="{res.style.wrapper}">
                <g:HTMLPanel tag="section" ui:field="bee" styleName="{page.style.popupContent}"/>
                <a href="#" ui:field="popupClose" class="{page.style.popupClose}">
                    <i class="{fonts.icons.iconClose}"></i>
                </a>
            </div>
        </div>

    </g:HTMLPanel>
</ui:UiBinder>
